                    </source-folder>
                    <source-folder style="packages">
                        <label>millr/src/benchmark</label>
                        <location>millr/src/benchmark</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>millr/src/old-test</label>
                        <location>millr/src/old-test</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>millr/src/test</label>
                        <location>millr/src/test</location>
                        <includes>**/*.java</includes>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="javadoc"/>
                    <ide-action name="test-mokapot"/>
                    <ide-action name="test-millr"/>
                    <ide-action name="profile"/>
                </context-menu>
            </view>
            <subprojects/>
