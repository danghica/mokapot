                    <location>common/src/main/java</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>mokapot/src/main/java</label>
                    <type>java</type>
                    <location>mokapot/src/main/java</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>mokapot/src/whitelist/java</label>
                    <type>java</type>
