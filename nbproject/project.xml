                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>millr/src/test</label>
                    <type>java</type>
                    <location>millr/src/test</location>
                    <includes>**/*.java</includes>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>millr/src/benchmark/java</label>
                    <type>java</type>
                    <location>millr/src/benchmark/java</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>millr/src/old-test</label>
                    <type>java</type>
                    <location>millr/src/old-test</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>compile-all-with-tests</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="javadoc">
                    <target>doc-private</target>
                </action>
                <action name="test-millr">
                    <target>test-mokapotsemantics</target>
                </action>
                <action name="test-mokapot">
                    <target>test-mokapot-light</target>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>compile-all-with-tests</target>
                </action>
                <action name="profile">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>profile-nb</target>
                </action>
                <action name="run.single">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>run-selected-file-in-java</target>
                    <context>
                        <property>run.class</property>
                        <folder>millr/src/test/java</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="test.single">
                    <script>build.xml</script>
                    <target>test-single</target>
                    <context>
                        <property>test.class</property>
                        <folder>millr/src/test/java</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
            </ide-actions>
            <export>
                <type>folder</type>
                <location>build-output/classes-common</location>
                <build-target>compile-all-with-tests</build-target>
            </export>
            <export>
                <type>folder</type>
                <location>build-output/classes-mokapot</location>
                <build-target>compile-all-with-tests</build-target>
            </export>
            <export>
                <type>folder</type>
                <location>build-output/classes-whitelist</location>
                <build-target>compile-all-with-tests</build-target>
            </export>
            <export>
                <type>folder</type>
                <location>build-output/classes-millr</location>
                <build-target>compile-all-with-tests</build-target>
            </export>
            <export>
                <type>folder</type>
                <location>build-internal/classes</location>
                <build-target>compile-all-with-tests</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>common/src/main/java</label>
                        <location>common/src/main/java</location>
                    </source-folder>
                    <source-folder style="packages">
